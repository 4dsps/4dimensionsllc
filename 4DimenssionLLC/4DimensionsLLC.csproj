<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <RootNamespace>_4DimenssionLLC</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="4dimsllc.db" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="4dimsllc.db">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <None Include="bin\Debug\4dimsllc.db" />
    <None Include="bin\Release\4dimsllc.db" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="System.Data.SQLite" Version="1.0.118" />
  </ItemGroup>

</Project>
