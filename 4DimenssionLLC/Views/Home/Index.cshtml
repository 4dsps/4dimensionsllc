	<script>
		// Select elements by class name 'green'
		var greenElements = document.getElementsByClassName('green');

		// Select elements by class name 'toggle-color-btn'
		var toggleColorButtons = document.getElementsByClassName('toggle-color-btn');

		// Loop through the 'green' elements and add event listener to each element
		for (var i = 0; i < greenElements.length; i++) {
			greenElements[i].addEventListener('click', function () {
				for (var j = 0; j < toggleColorButtons.length; j++) {
					if (toggleColorButtons[j].style.color === 'green') {
						toggleColorButtons[j].style.color = ''; // Reset to default color
					} else {
						toggleColorButtons[j].style.color = 'green'; // Set to green
					}
				}
			});
		}


		$(document).ready(function () {
		
			$("#lblContact").click(function () {
				if ($(window).width() < 768) { // Assuming 768 pixels is the threshold for mobile view
					$(".icon").click();
				}
			});

			$("#lblFleet").click(function () {
				if ($(window).width() < 768) {
					$(".icon").click();
				}
			});

			$("#lblHuman").click(function () {
				if ($(window).width() < 768) {
					$(".icon").click();
				}
			});

			$("#lblDispatch").click(function () {
				if ($(window).width() < 768) {
					$(".icon").click();
				}
			});
		});

		var acc = document.getElementsByClassName("accordion");
		var i;

		for (i = 0; i < acc.length; i++) {
			acc[i].addEventListener("click", function () {
				this.classList.toggle("active");
				var panel = this.nextElementSibling;
				if (panel.style.maxHeight) {
					panel.style.maxHeight = null;
				} else {
					panel.style.maxHeight = panel.scrollHeight + "px";
				}
			});
		}
		window.onscroll = function () { scrollFunction() };
		function scrollFunction() {
			if (document.body.scrollTop > 100 || document.documentElement.scrollTop > 100) {
				document.getElementById("navbar").style.top = "0";
				document.getElementById("scroll-up").style.display = "block";
			} else {
				document.getElementById("navbar").style.top = "-119px";
				document.getElementById("scroll-up").style.display = "none";
			}
		}

		function myFunction() {
			var x = document.getElementById("myTopnav");
			if (x.className === "topnav") {
				x.className += " responsive";
			} else {
				x.className = "topnav";
			}
		}
		//SCROLL ANIMATE
		var scroll = window.requestAnimationFrame ||
			function (callback) { window.setTimeout(callback, 1000 / 60) };
		var elementsToShow = document.querySelectorAll('.show-on-scroll');
		function loop() {

			Array.prototype.forEach.call(elementsToShow, function (element) {
				if (isElementInViewport(element)) {
					element.classList.add('is-visible');
				} else {
					element.classList.remove('is-visible');
				}
			});

			scroll(loop);
		}
		loop();

		function addActiveClass(event) {
			// Remove 'active' class from all sections
			var sections = document.querySelectorAll('section');
			sections.forEach(function (section) {
				section.classList.remove('active');
			});

			// Get the ID of the section to be made active
			var sectionId = event.target.getAttribute('href').substring(1); // Remove the '#' from the href
			var activeSection = document.getElementById(sectionId);

			// Add 'active' class to the section
			activeSection.classList.add('active');
		}

		// Add click event listeners to all links
		var links = document.querySelectorAll('.scroll-link');
		links.forEach(function (link) {
			link.addEventListener('click', addActiveClass);
		});

		// Function to handle scroll event
		function handleScroll() {
			var scrollPosition = window.scrollY;

			// Remove 'active' class from all sections
			var sections = document.querySelectorAll('section');
			sections.forEach(function (section) {
				var sectionTop = section.offsetTop;
				var sectionHeight = section.offsetHeight;

				// Add 'active' class to the section currently in view
				if (scrollPosition >= sectionTop && scrollPosition < sectionTop + sectionHeight) {
					section.classList.add('active');
				} else {
					section.classList.remove('active');
				}
			});
		}

		// Add scroll event listener
		window.addEventListener('scroll', handleScroll);

		function isElementInViewport(el) {
			// special bonus for those using jQuery
			if (typeof jQuery === "function" && el instanceof jQuery) {
				el = el[0];
			}
			var rect = el.getBoundingClientRect();
			return (
				(rect.top <= 0
					&& rect.bottom >= 0)
				||
				(rect.bottom >= (window.innerHeight || document.documentElement.clientHeight) &&
					rect.top <= (window.innerHeight || document.documentElement.clientHeight))
				||
				(rect.top >= 0 &&
					rect.bottom <= (window.innerHeight || document.documentElement.clientHeight))
			);
		}
		var myVar;
		function myLoader() {
			myVar = setTimeout(showPage, 3000);
		}

		function showPage() {
			document.getElementById("loader").style.display = "none";
			document.getElementById("myDiv").style.display = "block";
		}
		//email send script
		function sendEmail() {
			//alert("click");
			var name = document.getElementById("name").value;
			var email = document.getElementById("email").value;
			var message = document.querySelector('textarea[name="message"]').value;

			// Get the button element
			var sendButton = document.getElementById("submit");

			// Change the button text to "Sending Email"
			sendButton.textContent = "Sending Email";

			// Check if any field is empty
			if (name.trim() === '' || email.trim() === '' || message.trim() === '') {
				// Show toast message
				Toastify({
					text: "Please fill in all fields before sending the email.",
					duration: 3000,
					close: true,
					gravity: "top",
					position: "center",
					className: "toastify-warning",
				}).showToast();

				// Revert button text back to "Send"
				sendButton.textContent = "Submitting ...";

				return;
			}

			// Show loading indicator
			var loadingToast = Toastify({
				text: "Sending email...",
				duration: -1, // Indefinite duration
				close: false,
				gravity: "top",
				position: "center",
			}).showToast();

			// Make AJAX request
			$.post("/Home/SendEmail", {
				recipient: email,
				body: message
			}, function (data, status) {
				// Hide loading indicator if it exists
				if (loadingToast) {
					loadingToast.hideToast();
					loadingToast = null; // Remove reference to the loading toast
				}

				if (status) {
					Toastify({
						text: "An email has been sent to " + email + " successfully!",
						duration: 3000,
						close: true,
						gravity: "top",
						position: "center",
					}).showToast();
					document.getElementById("name").value = "";
					document.getElementById("email").value = "";
					document.querySelector('textarea[name="message"]').value = "";
				} else {
					Toastify({
						text: "Email could not be sent to " + email,
						duration: 3000,
						close: true,
						gravity: "top",
						position: "center",
					}).showToast();
				}

				// Revert button text back to "Send"
				sendButton.textContent = "Send";
			});
		}
		$(function () {
			$(".icon-set").click(function () {
				$(".icon-set, .txt-span").toggleClass("press", 1000);
			});
		});

		document.addEventListener('DOMContentLoaded', function () {
			const blockquotes = document.querySelectorAll('figure.snip1390 blockquote');

			blockquotes.forEach(blockquote => {
				const text = blockquote.innerHTML;
				blockquote.innerHTML = `${text}<span class="close-quote">”</span>`;
			});
		});

		
		const container = document.getElementById('our-team');

		data.team.forEach((profile, index) => {
			const figure = document.createElement('figure');
			figure.className = 'snip1390';

			figure.innerHTML = `
				  <img src="${profile.img}" alt="${profile.name}" class="profile" />
				  <figcaption>
					<h2>${profile.name}</h2>
					<h4>${profile.role}</h4>
					<blockquote>${profile.quote}</blockquote>
				  </figcaption>
				`;

			container.appendChild(figure);

			// Add a break element after every three profiles
			if ((index + 1) % 3 === 0) {
				const breakElement = document.createElement('div');
				breakElement.className = 'break';
				container.appendChild(breakElement);
			}
		});


		//crousel
		// Select DOM elements
		const indicatorsContainer = document.getElementById('carousel-indicators');
		const innerContainer = document.getElementById('carousel-inner');

		// Generate carousel indicators and items
		data.events.forEach((event, index) => {
			// Create indicator
			const indicator = document.createElement('button');
			indicator.type = 'button';
			indicator.setAttribute('data-bs-target', '#carouselExampleDark');
			indicator.setAttribute('data-bs-slide-to', index);
			if (index === 0) indicator.classList.add('active');
			indicator.setAttribute('aria-current', index === 0 ? 'true' : 'false');
			indicator.setAttribute('aria-label', `Slide ${index + 1}`);
			indicatorsContainer.appendChild(indicator);

			// Create carousel item
			const item = document.createElement('div');
			item.classList.add('carousel-item');
			if (index === 0) item.classList.add('active');

			const img = document.createElement('img');
			img.src = event.pics[0]; // Use the first image in the pics array
			img.classList.add('d-block', 'w-100');
			img.alt = event.name;

			const caption = document.createElement('div');
			caption.classList.add('carousel-caption', 'd-none', 'd-md-block');

			const title = document.createElement('h5');
			title.textContent = event.name;

			const description = document.createElement('p');
			description.textContent = event.remarks;

			caption.appendChild(title);
			caption.appendChild(description);

			item.appendChild(img);
			item.appendChild(caption);

			innerContainer.appendChild(item);
		});
	</script>
