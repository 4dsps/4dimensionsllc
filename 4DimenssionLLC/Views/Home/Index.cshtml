	<script>
		document.getElementById('green').addEventListener('click', function () {
			const icon = document.getElementById('toggle-color-btn');
			if (icon.style.color === 'green') {
				icon.style.color = ''; // Reset to default color
			} else {
				icon.style.color = 'green'; // Set to green
			}
		});
		$(document).ready(function () {
		
			$("#lblContact").click(function () {
				if ($(window).width() < 768) { // Assuming 768 pixels is the threshold for mobile view
					$(".icon").click();
				}
			});

			$("#lblFleet").click(function () {
				if ($(window).width() < 768) {
					$(".icon").click();
				}
			});

			$("#lblHuman").click(function () {
				if ($(window).width() < 768) {
					$(".icon").click();
				}
			});

			$("#lblDispatch").click(function () {
				if ($(window).width() < 768) {
					$(".icon").click();
				}
			});
		});

		var acc = document.getElementsByClassName("accordion");
		var i;

		for (i = 0; i < acc.length; i++) {
			acc[i].addEventListener("click", function () {
				this.classList.toggle("active");
				var panel = this.nextElementSibling;
				if (panel.style.maxHeight) {
					panel.style.maxHeight = null;
				} else {
					panel.style.maxHeight = panel.scrollHeight + "px";
				}
			});
		}
		window.onscroll = function () { scrollFunction() };
		function scrollFunction() {
			if (document.body.scrollTop > 100 || document.documentElement.scrollTop > 100) {
				document.getElementById("navbar").style.top = "0";
				document.getElementById("scroll-up").style.display = "block";
			} else {
				document.getElementById("navbar").style.top = "-119px";
				document.getElementById("scroll-up").style.display = "none";
			}
		}

		function myFunction() {
			var x = document.getElementById("myTopnav");
			if (x.className === "topnav") {
				x.className += " responsive";
			} else {
				x.className = "topnav";
			}
		}
		//SCROLL ANIMATE
		var scroll = window.requestAnimationFrame ||
			function (callback) { window.setTimeout(callback, 1000 / 60) };
		var elementsToShow = document.querySelectorAll('.show-on-scroll');
		function loop() {

			Array.prototype.forEach.call(elementsToShow, function (element) {
				if (isElementInViewport(element)) {
					element.classList.add('is-visible');
				} else {
					element.classList.remove('is-visible');
				}
			});

			scroll(loop);
		}
		loop();

		function addActiveClass(event) {
			// Remove 'active' class from all sections
			var sections = document.querySelectorAll('section');
			sections.forEach(function (section) {
				section.classList.remove('active');
			});

			// Get the ID of the section to be made active
			var sectionId = event.target.getAttribute('href').substring(1); // Remove the '#' from the href
			var activeSection = document.getElementById(sectionId);

			// Add 'active' class to the section
			activeSection.classList.add('active');
		}

		// Add click event listeners to all links
		var links = document.querySelectorAll('.scroll-link');
		links.forEach(function (link) {
			link.addEventListener('click', addActiveClass);
		});

		// Function to handle scroll event
		function handleScroll() {
			var scrollPosition = window.scrollY;

			// Remove 'active' class from all sections
			var sections = document.querySelectorAll('section');
			sections.forEach(function (section) {
				var sectionTop = section.offsetTop;
				var sectionHeight = section.offsetHeight;

				// Add 'active' class to the section currently in view
				if (scrollPosition >= sectionTop && scrollPosition < sectionTop + sectionHeight) {
					section.classList.add('active');
				} else {
					section.classList.remove('active');
				}
			});
		}

		// Add scroll event listener
		window.addEventListener('scroll', handleScroll);

		function isElementInViewport(el) {
			// special bonus for those using jQuery
			if (typeof jQuery === "function" && el instanceof jQuery) {
				el = el[0];
			}
			var rect = el.getBoundingClientRect();
			return (
				(rect.top <= 0
					&& rect.bottom >= 0)
				||
				(rect.bottom >= (window.innerHeight || document.documentElement.clientHeight) &&
					rect.top <= (window.innerHeight || document.documentElement.clientHeight))
				||
				(rect.top >= 0 &&
					rect.bottom <= (window.innerHeight || document.documentElement.clientHeight))
			);
		}
		var myVar;
		function myLoader() {
			myVar = setTimeout(showPage, 3000);
		}

		function showPage() {
			document.getElementById("loader").style.display = "none";
			document.getElementById("myDiv").style.display = "block";
		}
		//email send script
		function sendEmail() {
			//alert("click");
			var name = document.getElementById("name").value;
			var email = document.getElementById("email").value;
			var message = document.querySelector('textarea[name="message"]').value;

			// Get the button element
			var sendButton = document.getElementById("submit");

			// Change the button text to "Sending Email"
			sendButton.textContent = "Sending Email";

			// Check if any field is empty
			if (name.trim() === '' || email.trim() === '' || message.trim() === '') {
				// Show toast message
				Toastify({
					text: "Please fill in all fields before sending the email.",
					duration: 3000,
					close: true,
					gravity: "top",
					position: "center",
					className: "toastify-warning",
				}).showToast();

				// Revert button text back to "Send"
				sendButton.textContent = "Submitting ...";

				return;
			}

			// Show loading indicator
			var loadingToast = Toastify({
				text: "Sending email...",
				duration: -1, // Indefinite duration
				close: false,
				gravity: "top",
				position: "center",
			}).showToast();

			// Make AJAX request
			$.post("/Home/SendEmail", {
				recipient: email,
				body: message
			}, function (data, status) {
				// Hide loading indicator if it exists
				if (loadingToast) {
					loadingToast.hideToast();
					loadingToast = null; // Remove reference to the loading toast
				}

				if (status) {
					Toastify({
						text: "An email has been sent to " + email + " successfully!",
						duration: 3000,
						close: true,
						gravity: "top",
						position: "center",
					}).showToast();
					document.getElementById("name").value = "";
					document.getElementById("email").value = "";
					document.querySelector('textarea[name="message"]').value = "";
				} else {
					Toastify({
						text: "Email could not be sent to " + email,
						duration: 3000,
						close: true,
						gravity: "top",
						position: "center",
					}).showToast();
				}

				// Revert button text back to "Send"
				sendButton.textContent = "Send";
			});
		}
		$(function () {
			$(".icon-set").click(function () {
				$(".icon-set, .txt-span").toggleClass("press", 1000);
			});
		});

		

		
	</script>
